    "express": "^4.21.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.11",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-linkedin-oauth2": "^2.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.0"
  }
}

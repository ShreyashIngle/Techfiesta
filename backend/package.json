{
  "name": "yieldvision-backend",
  "version": "1.0.0",
  "description": "Backend for YieldVision application",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run start-node\" \"npm run start-fastapi\"",
